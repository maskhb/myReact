{
  "name": "react-framework-base",
  "version": "0.0.1",
  "description": "An out-of-box UI solution for enterprise applications",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "start": "cross-env ESLINT=none roadhog dev",
    "start:no-proxy": "cross-env NO_PROXY=true ESLINT=none roadhog dev",
    "build": "node ./tools/proxy && cross-env ESLINT=none roadhog build",
    "site": "roadhog-api-doc static && gh-pages -d dist",
    "analyze": "cross-env ANALYZE=true roadhog build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint:style:scss": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint": "eslint --ext .js src templates mock tests && npm run lint:style & npm run lint:style:scss",
    "lint:fix": "eslint --fix --ext .js src templates mock tests && npm run lint:style & npm run lint:style:scss",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "roadhog test",
    "test:component": "roadhog test ./src/components",
    "test:all": "node ./tests/run-tests.js"
  },
  "dependencies": {
    "@antv/data-set": "^0.8.0",
    "@babel/polyfill": "^7.0.0-beta.36",
    "antd": "^3.1.0",
    "area-data": "^1.0.0",
    "babel-runtime": "^6.9.2",
    "bizcharts": "^3.1.0-beta.4",
    "bizcharts-plugin-slider": "^2.0.1",
    "classnames": "^2.2.5",
    "crypto-js": "^3.1.9-1",
    "draft-js": "^0.10.5",
    "draftjs-to-html": "^0.8.3",
    "dva": "^2.1.0",
    "dva-loading": "^1.0.4",
    "enquire-js": "^0.1.1",
    "fastclick": "^1.0.6",
    "immutability-helper": "^2.6.6",
    "lodash": "^4.17.4",
    "lodash-decorators": "^4.4.1",
    "moment": "^2.19.1",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.0",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "rc-drawer-menu": "^0.5.0",
    "react": "^16.2.0",
    "react-color": "^2.14.0",
    "react-container-query": "^0.9.1",
    "react-cropper": "^1.0.1",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.2.0",
    "react-draft-wysiwyg": "^1.12.11",
    "react-fittext": "^1.0.0",
    "react-guard": "^0.5.0",
    "recompose": "^0.26.0",
    "url-polyfill": "^1.0.10",
    "utility": "^1.13.1"
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "babel-plugin-dva-hmr": "^0.4.1",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-import-glob-fix": "^2.0.0-fix",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-react-css-modules": "^3.3.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.1.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.0.1",
    "gh-pages": "^1.0.0",
    "htlib": "git+ssh://git@gitlab.hd:web/htlib.git#v1.0.2-htlib-20171102-1553-release",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "lodash-webpack-plugin": "^0.11.4",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.7.2",
    "postcss-scss": "^1.0.3",
    "pro-download": "^1.0.1",
    "raw-loader": "^0.5.1",
    "redbox-react": "^1.5.0",
    "regenerator-runtime": "^0.11.1",
    "require-glob": "^3.2.0",
    "roadhog": "^2.2.0",
    "roadhog-api-doc": "^0.3.4",
    "rollbar": "^2.3.4",
    "routes-loader": "^0.1.3",
    "sass-loader": "^6.0.6",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0"
  },
  "optionalDependencies": {
    "nightmare": "^2.10.0"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "lint-staged:js",
    "**/*.less": "stylelint --syntax less",
    "**/*.scss": "stylelint --syntax scss"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
